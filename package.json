{
    "name": "Polis",
    "version": "0.0.1",
    "description": "Polis is a real-time 3D viewer based on Cesium, that displays the buildings in and objects of interest near some base, with necessary metrics.",
    "homepage": "https://www.showingcloud.com",
    "license": "Apache-2.0",
    "main": "server.js",
    "scripts": {
        "start": "node_modules/.bin/webpack-dev-server --config webpack.config.js --open",
        "build": "node_modules/.bin/webpack --config webpack.config.js --display-modules"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ShowingCloud/Polis"
    },
    "dependencies": {
        "cesium": "^1.63.1",
        "compression": "^1.6.2",
        "express": "^4.15",
        "font-awesome": "^4.7.0",
        "mqtt": "^3.0.0",
        "yargs": "^15.0.2"
    },
    "engines": {
        "node": "0.10.x"
    },
    "devDependencies": {
        "bootstrap": "^4.4.1",
        "bootstrap-select": "^1.13.12",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.4.0",
        "eslint": "^6.1.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-react-hooks": "^1.7.0",
        "html-webpack-plugin": "^3.2.0",
        "html-webpack-template": "^6.2.0",
        "jquery": "^3.4.1",
        "popper.js": "^1.16.0",
        "requirejs": "^2.3.6",
        "style-loader": "^1.1.2",
        "url-loader": "^3.0.0",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1"
    },
    "eslintConfig": {
        "env": {
            "es6": true,
            "mongo": true,
            "node": true
        },
        "extends": "airbnb",
        "globals": {
            "Atomics": "readonly",
            "SharedArrayBuffer": "readonly"
        },
        "parserOptions": {
            "ecmaVersion": 2019,
            "sourceType": "module"
        },
        "rules": {}
    }
}
